        })   
         wait()
    
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
        local Window = Library.CreateLib("Bean Hub I Anime Clicker Fight", "Ocean")
      ---Main
        local Main = Window:NewTab("Main")
        local MainSection = Main:NewSection("Main")
        
        MainSection:NewToggle("AutoClick/KillAura", "Kills any enemny next to you", function(state)
            if state then
                _G.autoClick = true
while _G.autoClick == true do
    game:GetService("ReplicatedStorage").System.SystemClick.Click:FireServer()
task.wait()
end

game:GetService("ReplicatedStorage").System.SystemClick.Click:FireServer()

            else
                _G.autoClick = false
while _G.autoClick == true do
    game:GetService("ReplicatedStorage").System.SystemClick.Click:FireServer()
task.wait()
end

game:GetService("ReplicatedStorage").System.SystemClick.Click:FireServer()
            end
        end)

        MainSection:NewToggle("AutoFarm (Fast)", "AutoFarms", function(state)
            if state then

                _G.autofarm = true
                while _G.autofarm == true do
                    local CFrameEnd = CFrame.new(9612.07421875, 2.9995951652526855, -342.848388671875)
                    local Time = 30 ---seconds you want be there lol
                    local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
                    tween:Play()
                    tween.Completed:Wait()
                    task.wait()
                end            else
                print("Toggle Off")
            end
        end)
        MainSection:NewToggle("AutoFarm (Normal)", "AutoFarms", function(state)
            if state then

                _G.autofarm = true
                while _G.autofarm == true do
                    local CFrameEnd = CFrame.new(9612.07421875, 2.9995951652526855, -342.848388671875)
                    local Time = 70 ---seconds you want be there lol
                    local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
                    tween:Play()
                    tween.Completed:Wait()
                    task.wait()
                end            else
                print("Toggle Off")
            end
        end)

        MainSection:NewButton("Force Rest Character", "Kills", function()
            print("Clicked")
        end)

