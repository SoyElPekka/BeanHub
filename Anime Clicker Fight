        ---Player
        local Player = Window:NewTab("Player")
        local PlayerSection = Player:NewSection("Player")

        
        
    PlayerSection:NewSlider("WallkSpeed", "Changes Wallkspeed", 400, 16, function(v) 
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end)

    PlayerSection:NewToggle("Auto Dash", "Auto Dash", function(state)
    if state then
        _G.autoDash = true
        while _G.autoDash == true do
        local args = {
            [1] = {
                ["Goal"] = Vector3.new(23.395856857299805, 3.251938819885254, -332.7722473144531),
                ["PlayerId"] = 3915611938
            },
            [2] = "BloxFruitDash"
        }
        game:GetService("ReplicatedStorage").ServerMsg.ReleaseSkill:FireServer(unpack(args))
        task.wait()
        end
        
        
    else
        _G.autoDash = false
        while _G.autoDash == true do
        local args = {
            [1] = {
                ["Goal"] = Vector3.new(23.395856857299805, 3.251938819885254, -332.7722473144531),
                ["PlayerId"] = 3915611938
            },
            [2] = "BloxFruitDash"
        }
        game:GetService("ReplicatedStorage").ServerMsg.ReleaseSkill:FireServer(unpack(args))
        task.wait()
        end
        
        
    end
end)

